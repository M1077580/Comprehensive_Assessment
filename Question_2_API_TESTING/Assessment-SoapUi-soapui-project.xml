import jxl.write.*
import com.eviware.soapui.impl.wsdl.testcase.WsdlTestRunContext
import groovy.json.JsonSlurper

//Getting data from excel 
Workbook excelFile= Workbook.getWorkbook(new File("C:\\Users\\M1077580\\Desktop\\excelbook.xls"));
Sheet sheet = excelFile.getSheet(0)
Cell a1 = sheet.getCell(0,0)
Cell a2 = sheet.getCell(1,0)
Cell a3 = sheet.getCell(2,0)
Cell a4 = sheet.getCell(3,0)
def cmpt = a1.getContents(); 
def id = a2.getContents();
def name = a3.getContents();
def yoj =  a4.getContents();
excelFile.close()
//log.info cmpt 
//log.info id 
//log.info name 
//log.info yoj

def testCaseLocation = testRunner.testCase.testSuite.testCases["TestCase"];
//Setting Custom Properties
testCaseLocation.setPropertyValue("id", id);
testCaseLocation.setPropertyValue("name", name);
testCaseLocation.setPropertyValue("cmpt", cmpt);
testCaseLocation.setPropertyValue("yoj", yoj);

//Running Post Test Case
def postTestLocation = testCaseLocation.testSteps["post"]; 
def postContext = new WsdlTestRunContext(postTestLocation);
postTestLocation.run(testRunner, postContext);
def response = postTestLocation.getPropertyValue("response")
def jsonResponsePost = new JsonSlurper().parseText(response)
log.info jsonResponsePost.httpStatus

//
////Getting Employee Details By id to check
//

def getTestLocation = testCaseLocation.testSteps["getById"];
def getContext = new WsdlTestRunContext(getTestLocation);
getTestLocation.run(testRunner, getContext);
def responseGet = getTestLocation.getPropertyValue("response")
def jsonResponseGet = new JsonSlurper().parseText(responseGet)
log.info jsonResponseGet.httpStatus
assert id == jsonResponseGet.body.id.toString()
if(id.equals(jsonResponseGet.body.id.toString())){
	log.info "Get Data Matched Success" 
}

//
////Put Details to Update

//Updating custom Properties
testCaseLocation.setPropertyValue("name", name+"a");
testCaseLocation.setPropertyValue("cmpt", cmpt+"a");
testCaseLocation.setPropertyValue("yoj", yoj+"0");

//
def putTestLocation = testCaseLocation.testSteps["put"];
def putContext = new WsdlTestRunContext(putTestLocation);
putTestLocation.run(testRunner, putContext);
def responsePut = putTestLocation.getPropertyValue("response")
def jsonResponsePut = new JsonSlurper().parseText(responsePut)
log.info jsonResponsePut.httpStatus
//
//
////Deleting By Id
//
// 

def delTestLocation = testCaseLocation.testSteps["delete"];
def delContext = new WsdlTestRunContext(delTestLocation);
delTestLocation.run(testRunner, delContext);
def responseDel = delTestLocation.getPropertyValue("response")
def jsonResponseDel = new JsonSlurper().parseText(responseDel)
log.info jsonResponseDel.httpStatus

//Checking if deleted
def getAllTestLocation = testCaseLocation.testSteps["getAll"];
def getAllContext = new WsdlTestRunContext(getAllTestLocation);
getAllTestLocation.run(testRunner, getAllContext);
def responseGetAll = getAllTestLocation.getPropertyValue("response")
def jsonResponseGetAll = new JsonSlurper().parseText(responseGetAll)
//log.info jsonResponseGetAll.httpStatus

int cnt = 0
for(int i=0; i&lt;jsonResponseGetAll.body.size(); i++){
	if ( id.equals(jsonResponseGetAll.body[i].id.toString() ) ){
		log.info "Id Matched"
		cnt = cnt+1;
	}
}
if(cnt == 0){
	log.info "Id Not Foud, Deletion Successfull"
}



